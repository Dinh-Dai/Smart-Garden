    {
        "id": "08c84739995fe6bc",
        "type": "function",
        "z": "b9c8e7b072ef3e80",
        "name": "Pump Logic",
        "func": "const mode = flow.get('pump_mode') || 'MANUAL';\nconst soilMoisture = flow.get('soil_moisture') || 0;\n\nif (msg.topic === 'pump/mode') {\n    flow.set('pump_mode', msg.payload);\n    return null;\n}\n\nif (mode === 'AUTO') {\n    msg.payload = soilMoisture < 30 ? 'ON' : 'OFF';\n}\n\nreturn msg;",
        "outputs": 1,
        "x": 570,
        "y": 780,
        "wires": [
